{
    "name": "Scripting Extension",
    "manifest_version": 3,
    "permissions": ["contextMenu", "tabs"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": [],
            "js": ["./content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
}